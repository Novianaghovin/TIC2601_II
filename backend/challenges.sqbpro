ot_y_axes/><global_filter/></table><table schema="main" name="User_Profile" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="52"/><column index="2" value="35"/><column index="3" value="48"/><column index="4" value="44"/><column index="5" value="46"/><column index="6" value="69"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="User_Registration" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="50"/><column index="2" value="70"/><column index="3" value="68"/><column index="4" value="39"/><column index="5" value="62"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE User_Registration (
    user_id INTEGER PRIMARY KEY AUTOINCREMENT, -- Unique identifier for each user
    first_name VARCHAR(50) NOT NULL,       -- User's first name
    last_name VARCHAR(50) NOT NULL,        -- User's last name
    email VARCHAR(100) NOT NULL UNIQUE,     -- User's email address
    password VARCHAR(255) NOT NULL          -- User's password (hashed)
);

CREATE TABLE User_Profile (
    user_id INTEGER PRIMARY KEY AUTOINCREMENT, -- Unique identifier for each user (Foreign Key)
    dob DATE,                                -- Date of birth
    gender VARCHAR(10),                      -- User's gender
    height INTEGER,                          -- User's height in cm
    weight INTEGER,                          -- User's weight in kg
    nationality VARCHAR(50),                 -- User's nationality
    FOREIGN KEY (user_id) REFERENCES User_Registration(user_id)  -- Reference to User Registration
);

CREATE TABLE User_Challenges (
    user_challenge_id INTEGER PRIMARY KEY AUTOINCREMENT, -- Unique identifier for the user challenge record
    user_id INTEGER NOT NULL,               -- Foreign key referencing the user
    challenge_id INTEGER NOT NULL,          -- Foreign key referencing the challenge
    completion_status VARCHAR(50),          -- Status of completion (e.g., Active, Completed, Inactive)
    progress INTEGER,                       -- Progress made by the user in the challenge
    start_date DATE,                        -- Date when the user started the challenge
    end_date DATE,                          -- Date when the user completed the challenge
    FOREIGN KEY (user_id) REFERENCES User_Registration(user_id),         -- Reference to the Users table
    FOREIGN KEY (challenge_id) REFERENCES Challenges(challenge_id)  -- Reference to the Challenges table
);

